<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 加载配置文件 -->
	<properties resource="db.properties"/>

	<context id="generator" targetRuntime="MyBatis3">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 数据库连接信息 -->
		<jdbcConnection driverClass="${jdbc.driver}"
			connectionURL="${jdbc.url}" userId="${jdbc.username}"
			password="${jdbc.password}">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- targetProject:生成PO类的位置 -->
		<javaModelGenerator targetPackage="${project.po}"
			targetProject=".\src">
			<!-- enableSubPackages:是否让Schema作为包的后缀 -->
			<property name="enableSubPackages" value="true" />
			<!-- trimStrings:从数据库返回的值被清理前后的空格 -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- targetProject:mapper映射文件生成的位置 -->
		<sqlMapGenerator targetPackage="${project.mapper}"
			targetProject=".\src">
			<!-- enableSubPackages:是否让Schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<!-- targetPackage:mapper接口的位置 -->
		<javaClientGenerator targetPackage="${project.mapper}"
			type="XMLMAPPER" targetProject=".\src">
			<!-- enableSubPackages:是否让Schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>
		<!-- 指定数据库表 -->
		<table tableName="${table.t1}"></table>
		<table tableName="${table.t2}"></table>
		<table tableName="${table.t3}"></table>
		<table tableName="${table.t4}"></table>
		<table tableName="${table.t5}"></table>
		<table tableName="${table.t6}"></table>
		<table tableName="${table.t7}"></table>
		<table tableName="${table.t8}"></table>
		<table tableName="${table.t9}"></table>
		<table tableName="${table.t10}"></table>
		<table tableName="${table.t11}"></table>
		<table tableName="${table.t12}"></table>
		<table tableName="${table.t13}"></table>
		<table tableName="${table.t14}"></table>
		<table tableName="${table.t15}"></table>
		<table tableName="${table.t16}"></table>
		<table tableName="${table.t17}"></table>
		<table tableName="${table.t18}"></table>
		<table tableName="${table.t19}"></table>
		<table tableName="${table.t20}"></table>
		<table tableName="${table.t21}"></table>
		<table tableName="${table.t22}"></table>
		<table tableName="${table.t23}"></table>
		<table tableName="${table.t24}"></table>
		<table tableName="${table.t25}"></table>
		<table tableName="${table.t26}"></table>
		<table tableName="${table.t27}"></table>
		<table tableName="${table.t28}"></table>
		<table tableName="${table.t29}"></table>
